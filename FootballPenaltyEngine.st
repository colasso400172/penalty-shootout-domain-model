Object subclass: #Billetera	instanceVariableNames: 'transacciones'	classVariableNames: ''	package: 'PPR-TP1-2024'!!Billetera methodsFor: 'control' stamp: 'colo 9/12/2024 17:16'!asString  	|mostrarBilletera|	mostrarBilletera := ''.	transacciones do:[:unaTransaccion | mostrarBilletera := mostrarBilletera, unaTransaccion asString, String cr ].	^mostrarBilletera ! !!Billetera methodsFor: 'control' stamp: 'JuanMauroBottari 9/20/2024 22:08'!posicionFinanciera		|final|		final := 0.		transacciones do:[:puntero | (puntero tipo = 'crédito') ifTrue: [ final := final + puntero importeFinal ] ifFalse: [ final := final - puntero importeFinal ]].		^final! !!Billetera methodsFor: 'control' stamp: 'colo 9/12/2024 17:49'!cantidadInmediata|inmediatas| inmediatas := transacciones select: [:unaTransaccion| (unaTransaccion isKindOf: TransaccionInmediata)].^inmediatas size! !!Billetera methodsFor: 'adding' stamp: 'colo 9/12/2024 17:33'!add: nuevaTransaccion	(nuevaTransaccion isKindOf: Transaccion)	ifTrue: [ transacciones add: nuevaTransaccion ]  ! !!Billetera methodsFor: 'inicio' stamp: 'colo 9/12/2024 16:55'!initialize 	transacciones := Set new ! !Object subclass: #Transaccion	instanceVariableNames: 'monto fecha tipo'	classVariableNames: ''	package: 'PPR-TP1-2024'!!Transaccion methodsFor: 'inicio' stamp: 'colo 9/12/2024 13:23'!initialize 			fecha := 0.			monto := 0.			tipo := ''.									! !!Transaccion methodsFor: 'acceso' stamp: 'colo 9/12/2024 12:37'!tipo	^ tipo! !!Transaccion methodsFor: 'acceso' stamp: 'colo 9/12/2024 12:37'!monto: anObject	monto := anObject! !!Transaccion methodsFor: 'acceso' stamp: 'colo 9/12/2024 12:37'!fecha	^ fecha! !!Transaccion methodsFor: 'acceso' stamp: 'colo 9/12/2024 12:37'!monto	^ monto! !!Transaccion methodsFor: 'acceso' stamp: 'colo 9/12/2024 12:37'!fecha: anObject	fecha := anObject! !!Transaccion methodsFor: 'acceso' stamp: 'colo 9/12/2024 12:37'!tipo: anObject	tipo := anObject! !!Transaccion methodsFor: 'control' stamp: 'JuanMauroBottari 9/20/2024 22:04'!asString 	|mostrar|	mostrar := 'Fecha: ',	fecha asString, ' Monto: ', monto asString, ' Tipo: ', tipo asString.	^mostrar.		"Otra opción:	^('Fecha: ',	fecha asString, ' Monto: ', monto asString, ' Tipo: ', tipo asString.)	"! !Transaccion subclass: #TransaccionInmediata	instanceVariableNames: 'token'	classVariableNames: ''	package: 'PPR-TP1-2024'!!TransaccionInmediata methodsFor: 'inicio' stamp: 'JuanMauroBottari 9/20/2024 22:06'!initialize 		super initialize. 		"No es requerido inicializar token en 'nil' porque ya tiene ese valor."		token:= nil.		! !!TransaccionInmediata methodsFor: 'acceso' stamp: 'colo 9/12/2024 14:37'!token: anObject	token := anObject! !!TransaccionInmediata methodsFor: 'acceso' stamp: 'colo 9/12/2024 14:37'!token	^ token! !!TransaccionInmediata methodsFor: 'control' stamp: 'JuanMauroBottari 9/20/2024 22:04'!asString 	|mostrarInmediata|	mostrarInmediata := super asString,  ' Token: ', token asString.	^mostrarInmediata.	! !!TransaccionInmediata methodsFor: 'control' stamp: 'colo 9/12/2024 15:14'!importeFinal	|importe|	importe := 0.	self token ifNotNil:[importe:=self monto].	^importe.! !Transaccion subclass: #TransaccionPosdatada	instanceVariableNames: 'fechaPosdatado fechaAnulacion'	classVariableNames: ''	package: 'PPR-TP1-2024'!!TransaccionPosdatada methodsFor: 'control' stamp: 'colo 9/12/2024 16:44'!asString 		|mostrarPosdatado|		mostrarPosdatado := super asString, ' Fecha posdatado: ', fechaPosdatado asString, ' Fecha Anulacion: ', fechaAnulacion asString.		^ mostrarPosdatado.		! !!TransaccionPosdatada methodsFor: 'control' stamp: 'colo 9/12/2024 16:44'!cambiarFecha: unaFecha 		(unaFecha < fechaPosdatado) ifTrue: [ fechaAnulacion := unaFecha. ^ true]  ifFalse: [ ^ false ]				! !!TransaccionPosdatada methodsFor: 'control' stamp: 'colo 9/12/2024 16:41'!importeFinal	fechaAnulacion ifNil: [ ^monto ] ifNotNil: [ ^0 ]		! !!TransaccionPosdatada methodsFor: 'acceso' stamp: 'colo 9/12/2024 15:23'!fechaAnulacion	^ fechaAnulacion! !!TransaccionPosdatada methodsFor: 'acceso' stamp: 'colo 9/12/2024 15:23'!fechaAnulacion: anObject	fechaAnulacion := anObject! !!TransaccionPosdatada methodsFor: 'acceso' stamp: 'colo 9/12/2024 15:23'!fechaPosdatado	^ fechaPosdatado! !!TransaccionPosdatada methodsFor: 'acceso' stamp: 'colo 9/12/2024 15:23'!fechaPosdatado: anObject	fechaPosdatado := anObject! !!TransaccionPosdatada methodsFor: 'inicio' stamp: 'JuanMauroBottari 9/20/2024 22:07'!initialize 		super initialize. 		"No es requerido inicializar token en 'nil' porque ya tiene ese valor."		fechaAnulacion := nil.		fechaPosdatado := 0.		! !